{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "Localhost:3000",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}",
            "breakOnLoad": true,
            "sourceMaps": true,
            "sourceMapPathOverrides": {
                "webpack:///assets/js/*": "${webRoot}/assets/js/*",
                "webpack:///components/*": "${webRoot}/components/*",
                "webpack:///pages/*": "${webRoot}/pages/*",
                "webpack:///layouts/*": "${webRoot}/layouts/*",
                "webpack:///middleware/*": "${webRoot}/middleware/*",
                "webpack:///plugins/*": "${webRoot}/plugins/*",
                "webpack:///store/*": "${webRoot}/store/*",
                "webpack:///utils/*": "${webRoot}/utils/*"
            }
        }
    ]
}
